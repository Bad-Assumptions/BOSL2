{
    "problemMatcher": [
        {
            "owner": "openscad_docsgen",
            "pattern": [
                {
                    "regexp": "^!! (WARNING|ERROR) at ([^:]+):(\\d+): (.*)$",
                    "severity": 1,
                    "file": 2,
                    "line": 3,
                    "message": 4
                },
                {
                    "regexp": "^     (.*)$",
                    "message": 1,
                    "loop": true
                }
            ]
        }
    ]
}
